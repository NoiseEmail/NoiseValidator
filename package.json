    "@types/node": "^20.11.20",
    "bun-types": "^1.0.6",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "email-validator-pro": "^1.0.1",
    "fastify": "^4.26.1",
    "mergician": "^2.0.0"
